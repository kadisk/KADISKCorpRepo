{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"entryPort": {
			"type": "string"
		},
		"routeMappingTable": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"host": {
						"type": "string"
					},
					"target": {
						"type": "string"
					}
				},
				"required": [
					"host",
					"target"
				]
			}
		}
	},
	"required": [
		"entryPort",
		"routeMappingTable"
	]
}