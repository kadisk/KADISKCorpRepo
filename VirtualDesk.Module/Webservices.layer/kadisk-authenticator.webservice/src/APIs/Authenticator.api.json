{
	"name": "Authenticator",
	"endpoints": [
		{
			"path": "/authenticate",
			"method": "POST",
			"summary": "Authenticate",
			"parameters": [
				{
					"name": "username",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "password",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/logout",
			"method": "POST",
			"summary": "Logout"
		}
	]
}