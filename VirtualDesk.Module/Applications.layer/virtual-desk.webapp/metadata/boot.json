{
    "params" : [
        "port", 
        "serverName",
        "serverManagerUrl",
        "RT_ENV_GENERATED_DIR_NAME",
        "isWatch",
        "userManagementServiceStorageFilePath",
        "eventManagerServiceStorageFilePath",
        "instanceMonitoringManagerServiceStorageFilePath",
        "myWorkspaceManagerServiceStorageFilePath",
        "repositoryStorageFilePath",
        "serviceStorageFilePath",
        "myWorkspaceRepositoriesSourceCodeDirPath",
        "myServicesManagerImportedSourceCodeDirPath",
        "myServicesRepositoriesUploadDirPath",
        "myServicesInstanceDataDirPath",
        "myWorkbenchRepositoriesUploadDirPath",
        "installDataDirPath",
        "ecosystemDefaultsFileRelativePath",
        "secretKey"
    ],
    "services": [
        {
            "namespace": "@@/event-hub-service",
            "dependency": "@/event-manager.service/services/EventHubService",
            "params":{
                "storageFilePath": "{{eventManagerServiceStorageFilePath}}"                
            }
        },
        {
            "namespace": "@@/server-service",
            "dependency": "@/server-manager.service/services/HTTPServerService",
            "params":{
                "name":"{{serverName}}",
                "port": "{{port}}"
            },
            "bound-params":{
                "authenticationService": "@@/jwt-authentication-service"
            }
        },
        {
            "namespace": "@@/jwt-authentication-service",
            "dependency": "@/server-manager.service/services/JWTAuthenticationService",
            "params":{
                "secretKey":"{{secretKey}}"
            }
        },
        {
            "namespace": "@@/user-management-service",
            "dependency": "@/user-management.service/services/UserManagementService",
            "params":{
                "storageFilePath":"{{userManagementServiceStorageFilePath}}",
                "secretKey":"{{secretKey}}"
            }
        },
        {
            "namespace": "@@/ecosystemdata-handler-service",
            "dependency": "@/ecosystem-control-panel.service/services/EcosystemDataHandlerService",
            "params":{
                "installDataDirPath": "{{installDataDirPath}}"
            }
        },
        {
            "namespace": "@@/instance-monitoring-manager-service",
            "dependency": "@/instance-monitoring.service/services/InstanceMonitoringManager",
            "params":{
                "ecosystemDefaultsFileRelativePath":"{{ecosystemDefaultsFileRelativePath}}",
                "storageFilePath": "{{instanceMonitoringManagerServiceStorageFilePath}}"
            },
            "bound-params":{
                "ecosystemdataHandlerService": "@@/ecosystemdata-handler-service",
                "jsonFileUtilitiesLib": "@/json-file-utilities.lib",
                "supervisorLib": "@/supervisor.lib"
            }
        },
        {
            "namespace": "@@/my-workspace-manager-service",
            "dependency": "@/my-workbench.service/services/MyWorkspaceManager",
            "params":{
                "ecosystemDefaultsFileRelativePath":"{{ecosystemDefaultsFileRelativePath}}"
            },
            "bound-params":{
                "jsonFileUtilitiesLib": "@/json-file-utilities.lib",
                "loadMetatadaDirLib": "@/load-metatada-dir.lib",
                "ecosystemdataHandlerService": "@@/ecosystemdata-handler-service",
                "repositoryStorageManagerService": "@@/repository-storage-manager-service"
            }
        },
        {
            "namespace": "@@/container-manager-service",
            "dependency": "@/container-manager.service/services/ContainerManager"
        },
        {
            "namespace": "@@/my-services-manager-service",
            "dependency": "@/my-services.service/services/MyServicesManager",
            "bound-params":{
                "containerManagerService":"@@/container-manager-service",
                "repositoryStorageManagerService": "@@/repository-storage-manager-service"
            },
            "params":{
                "serviceStorageFilePath": "{{serviceStorageFilePath}}",
                "instanceDataDirPath": "{{myServicesInstanceDataDirPath}}"
            }
        },
        {
            "namespace": "@@/repository-storage-manager-service",
            "dependency": "@/repository-storage-manager.service/services/RepositoryStorageManager",
            "params":{
                "ecosystemDefaultsFileRelativePath": "{{ecosystemDefaultsFileRelativePath}}",
                "importedRepositoriesSourceCodeDirPath": "{{myServicesManagerImportedSourceCodeDirPath}}",
                "repositoryStorageFilePath": "{{repositoryStorageFilePath}}"
            },
            "bound-params":{
                "extractTarGzLib":"@/extract-tar-gz.lib",
                "loadMetatadaDirLib": "@/load-metatada-dir.lib",
                "ecosystemdataHandlerService": "@@/ecosystemdata-handler-service",
                "jsonFileUtilitiesLib": "@/json-file-utilities.lib"
            }
        }
    ],
    "endpoints": [
        {
            "dependency": "@/server-manager.webservice/endpoint-group",
            "bound-params":{
               "serverServiceForController":"@@/server-service",
               "serverService": "@@/server-service"
            }
        },
        {
            "dependency": "@/virtual-desk.webgui/endpoint-group",
            "params":{
                "serverManagerUrl":"{{serverManagerUrl}}",
                "serverName":"{{serverName}}",
                "RT_ENV_GENERATED_DIR_NAME":"{{RT_ENV_GENERATED_DIR_NAME}}",
                "isWatch": "isWatch"
            },
            "bound-params":{
                "serverService": "@@/server-service"
            }
        },
        {
            "dependency": "@/user-management.webservice/endpoint-group",
            "params":{
                "needsAuth": true
            },
            "bound-params":{
                "serverService": "@@/server-service",
                "userManagementService": "@@/user-management-service"
            }
        },
        {
            "dependency": "@/authenticator.webservice/endpoint-group",
            "params":{
                "needsAuth": true
            },
            "bound-params":{
                "serverService": "@@/server-service",
                "userManagementService": "@@/user-management-service"
            }
        },
        {
            "dependency": "@/ecosystem-administrator.webservice/endpoint-group",
            "bound-params":{
                "serverService": "@@/server-service",
                "ecosystemdataHandlerService": "@@/ecosystemdata-handler-service",
                "jsonFileUtilitiesLib": "@/json-file-utilities.lib",
                "ecosystemInstallUtilitiesLib": "@/ecosystem-install-utilities.lib",
                "eventHubService": "@@/event-hub-service",
                "containerManagerService":"@@/container-manager-service"
            },
            "params":{
                "ecosystemDefaultsFileRelativePath": "{{ecosystemDefaultsFileRelativePath}}",
                "needsAuth": true
            }
        },
        {
            "dependency": "@/ecosystem-monitoring.webservice/endpoint-group",
            "bound-params":{
                "serverService": "@@/server-service",
                "eventHubService": "@@/event-hub-service",
                "instanceMonitoringManagerService": "@@/instance-monitoring-manager-service"
            },
            "params":{
                "ecosystemDefaultsFileRelativePath": "{{ecosystemDefaultsFileRelativePath}}",
                "needsAuth": true
            }
        },
        {
            "dependency": "@/my-workbench.webservice/endpoint-group",
            "bound-params":{
                "serverService": "@@/server-service",
                "myWorkspaceManagerService":"@@/my-workspace-manager-service",
                "containerManagerService":"@@/container-manager-service",
                "repositoryStorageManagerService": "@@/repository-storage-manager-service"
            },
            "params":{
                "uploadDirPath": "{{myWorkbenchRepositoriesUploadDirPath}}",
                "needsAuth": true
            }
        },
        {
            "dependency": "@/my-services.webservice/endpoint-group",
            "bound-params":{
                "serverService": "@@/server-service",
                "myServicesManagerService": "@@/my-services-manager-service",
                "repositoryStorageManagerService": "@@/repository-storage-manager-service"
            },
            "params":{
                "uploadDirPath":"{{myServicesRepositoriesUploadDirPath}}",
                "needsAuth": true
            }
        }
        
    ]
}