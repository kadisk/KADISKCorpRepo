{
	"name": "ServiceProvisioning",
	"endpoints": [
		{
			"path": "/bootable-packages",
			"method": "GET",
			"summary": "ListBootablePackages"
		},
		{
			"path": "/provision-service/package",
			"method": "POST",
			"summary":"ProvisionService",
			"parameters": [
				{
					"in": "body",
					"name": "packageId",
					"type": "number",
					"required": true
				},
				{
					"in": "body",
					"name": "serviceName",
					"type": "string",
					"required": true
				},
				{
					"in": "body",
					"name": "serviceDescription",
					"type": "string",
					"required": true
				},
				{
					"in": "body",
					"name": "startupParams",
					"type": "json",
					"required": true
				},
				{
					"in": "body",
					"name": "ports",
					"type": "json",
					"required": true
				}
			]
		},
		{
			"path": "/provision-service/startup-params/data/:packageId",
			"method": "GET",
			"summary":"GetStartupParamsData",
			"parameters": [
				{
					"name": "packageId",
					"in": "path",
					"type": "number",
					"required": true
				}
			]
		}
	]
}