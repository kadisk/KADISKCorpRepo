{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"port": {
			"type": "string",
			"pattern": "^[0-9]+$"
		},
		"serverName": {
			"type": "string"
		},
		"ecosystemDefaultsFileRelativePath": {
			"type": "string"
		},
		"instanceMonitoringManagerServiceStorageFilePath": {
			"type": "string"
		},
		"installDataDirPath":{
			"type": "string"
		}
	},
	"required": [
		"port",
		"serverName",
		"ecosystemDefaultsFileRelativePath",
		"instanceMonitoringManagerServiceStorageFilePath",
		"installDataDirPath"
	]
}