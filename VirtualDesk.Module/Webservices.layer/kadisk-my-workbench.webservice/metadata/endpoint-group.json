{
    "bound-params":[
        "serverService",
        "myWorkspaceManagerService"
    ],
    "endpoints": [
        {
            "url": "/my-workspace",
            "type": "controller",
            "params": {
                "api-template": "APIs/MyWorkspace.api.json",
                "controller": "Controllers/MyWorkspace.controller"
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "myWorkspaceManagerService": "myWorkspaceManagerService"
                }
            }
        },
        {
            "url": "/my-ecosystem-console",
            "type": "controller",
            "params": {
                "api-template": "APIs/MyEcosystemConsole.api.json",
                "controller": "Controllers/MyEcosystemConsole.controller"
            },
            "bound-params": {
                "serverService":"serverService"
            }
        }
    ]
}