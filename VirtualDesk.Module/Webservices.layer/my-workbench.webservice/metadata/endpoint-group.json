{
    "bound-params":[
        "serverService",
        "myWorkspaceManagerService",
        "repositoryStorageManagerService",
        "containerManagerService"
    ],
    "params": [ "needsAuth", "uploadDirPath" ],
    "endpoints": [
        {
            "url": "/my-workspace",
            "type": "controller",
            "params": {
                "api-template": "APIs/MyWorkspace.api.json",
                "controller": "Controllers/MyWorkspace.controller",
                "needsAuth": "needsAuth",
                "controller-params":{
                    "uploadDirPath": "{{uploadDirPath}}"
                }
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "repositoryStorageManagerService": "repositoryStorageManagerService",
                    "myWorkspaceManagerService": "myWorkspaceManagerService"
                }
            }
        },
        {
            "url": "/my-ecosystem-console",
            "type": "controller",
            "params": {
                "api-template": "APIs/MyEcosystemConsole.api.json",
                "controller": "Controllers/MyEcosystemConsole.controller",
                "needsAuth": "needsAuth"
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "containerManagerService": "containerManagerService"
                }
            }
        }
    ]
}