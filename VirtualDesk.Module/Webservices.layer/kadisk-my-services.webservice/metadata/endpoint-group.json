{
    "bound-params":[
        "serverService"
    ],
    "params": [ "needsAuth", "uploadDirPath" ],
    "endpoints": [
        {
            "url": "/repository-import-manager",
            "type": "controller",
            "params": {
                "api-template": "APIs/RepositoryImportManager.api.json",
                "controller": "Controllers/RepositoryImportManager.controller",
                "needsAuth": "needsAuth",
