{
	"name": "ContainerManager",
	"endpoints": [
		{
			"path": "/containers",
			"method": "GET",
			"summary": "ListContainers"
		},
		{
			"summary": "RemoveContainer",
			"method": "POST",
			"path": "/remove-container",
			"parameters": [
				{
					"in": "body",
					"name": "containerIdOrName",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "StartContainer",
			"method": "POST",
			"path": "/start-container",
			"parameters": [
				{
					"in": "body",
					"name": "containerIdOrName",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"summary": "StopContainer",
			"method": "POST",
			"path": "/stop-container",
			"parameters": [
				{
					"in": "body",
					"name": "containerIdOrName",
					"type": "string",
					"required": true
				}
			]
		}
	]
}