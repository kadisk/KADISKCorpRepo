{
	"name": "MyServicesManager",
	"endpoints": [
		{
			"path": "/status",
			"method": "GET",
			"summary": "GetMyServicesStatus"
		},
		{
			"path": "/upload-repository",
			"method": "POST",
			"summary": "UploadRepository",
			"isMultipartFormData": true,
			"forwardRawRequest": true,
			"parameters": [
				{
					"in": "body",
					"name": "repositoryNamespace",
					"type": "string",
					"required": true
				},
				{
					"in": "body",
					"name": "repositoryFile",
					"type": "file",
					"required": true
				}
			]
		},
		{
			"path": "/provisioned-services",
			"method": "GET",
			"summary": "ListProvisionedServices"
		},
		{
			"path": "/service/data",
			"method": "GET",
			"summary": "GetServiceData",
			"parameters": [
				{
					"name": "serviceId",
					"in": "query",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/service/image-build-history",
			"method": "GET",
			"summary": "ListImageBuildHistory",
			"parameters": [
				{
					"name": "serviceId",
					"in": "query",
					"type": "string",
					"required": true
				}
			]
		},
{
			"path": "/service/instances",
			"method": "GET",
			"summary": "GetInstances",
			"parameters": [
				{
					"name": "serviceId",
					"in": "query",
					"type": "string",
					"required": true
				}
			]
		}
	]
}