{
    "bound-params":[
        "serverService",
        "myServicesManagerService",
        "repositoryStorageManagerService"
    ],
    "params": [ "needsAuth", "uploadDirPath" ],
    "endpoints": [
        {
            "url": "/my-service-manager",
            "type": "controller",
            "params": {
                "api-template": "APIs/MyServicesManager.api.json",
                "controller": "Controllers/MyServicesManager.controller",
                "needsAuth": "needsAuth"
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "myServicesManagerService":"myServicesManagerService"
                }
            }
        },
        {
            "url": "/repository-service-manager",
            "type": "controller",
            "params": {
                "api-template": "APIs/RepositoryServiceManager.api.json",
                "controller": "Controllers/RepositoryServiceManager.controller",
                "needsAuth": "needsAuth",
                "controller-params":{
                    "uploadDirPath": "{{uploadDirPath}}"
                }
            },
            "bound-params": {
                "serverService":"serverService",
                "controller-params":{
                    "repositoryStorageManagerService":"repositoryStorageManagerService"
                }
            }
        }  
    ]
}