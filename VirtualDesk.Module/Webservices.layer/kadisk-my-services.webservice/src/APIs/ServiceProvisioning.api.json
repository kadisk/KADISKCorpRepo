{
	"name": "ServiceProvisioning",
	"endpoints": [
		{
			"path": "/bootable-packages",
			"method": "GET",
			"summary": "ListBootablePackages"
		},
		{
			"path": "/applications",
			"method": "GET",
			"summary": "ListApplications"
		},
		{
			"path": "/provision-service/package",
			"method": "POST",
			"summary":"ProvisionServiceFromPackage",
			"parameters": [
				{
					"in": "body",
					"name": "packageId",
					"type": "number",
					"required": true
				}
			]
		},
		{
			"path": "/provision-service/application",
			"method": "POST",
			"summary":"ProvisionServiceFromApplication",
			"parameters": [
				{
					"in": "body",
					"name": "repositoryId",
					"type": "number",
					"required": true
				},
				{
					"in": "body",
					"name": "executableName",
					"type": "string",
					"required": true
				},
				{
					"in": "body",
					"name": "packagePath",
					"type": "string",
					"required": true
				}				
			]
		}
	]
}