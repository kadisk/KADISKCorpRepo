{
	"name": "MyWorkspace",
	"endpoints": [
		{
			"path": "/repositories",
			"method": "GET",
			"summary": "ListRepositories"
		},
		{
			"path": "/repository/create",
			"method": "POST",
			"summary": "CreateNewRepository",
			"parameters": [
				{
					"name": "repositoryNamespace",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/repository/import",
			"method": "POST",
			"summary": "ImportRepository",
			"parameters": [
				{
					"name": "repositoryNamespace",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "sourceCodeURL",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/repository-editor/hierarchy/:repositoryId",
			"method": "GET",
			"summary": "GetItemHierarchy",
			"parameters": [
				{
					"name": "repositoryId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/repository-editor/information/repository/:repositoryId",
			"method": "GET",
			"summary": "GetRepositoryGeneralInformation",
			"parameters": [
				{
					"name": "repositoryId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
			
		},
		{
			"path": "/repository-editor/information/item/:itemId",
			"method": "GET",
			"summary": "GetItemInformation",
			"parameters": [
				{
					"name": "itemId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
			
		},
		{
			"path": "/repository-editor/package/source-tree/:itemId",
			"method": "GET",
			"summary": "GetPackageSourceTree",
			"parameters": [
				{
					"name": "itemId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
			
		},
		{
			"path": "/repository-editor/applications-metadata/:repositoryId",
			"method": "GET",
			"summary": "GetApplicationsMetatadata",
			"parameters": [
				{
					"name": "repositoryId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
			
		}
	]
}